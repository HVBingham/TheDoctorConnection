@model IEnumerable<PrescriptionCapstone.Models.Log>
@{
    ViewBag.Title = "PatientsLog";
}

<h2>PatientsLog</h2>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.LogNotes)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
{
<tr>
    <td>
        @item.Time
    </td>
    <td>
        @item.Text
    </td>  
</tr>
}
</table>
